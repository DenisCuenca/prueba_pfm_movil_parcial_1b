<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeTrue">

    <Button
        android:id="@+id/btnfotos"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="128dp"
        android:layout_height="53dp"
        android:layout_marginStart="89dp"
        android:layout_marginTop="110dp"
        android:layout_marginEnd="4dp"
        android:background="#098FBA"
        android:text="FOTOS"
        app:layout_constraintEnd_toStartOf="@+id/button3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button3"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="165dp"
        android:layout_height="51dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="110dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="15dp"
        android:background="#098FBA"
        android:text="GEOLOCALIZACION"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnfotos"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button4"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="128dp"
        android:layout_height="50dp"
        android:layout_marginStart="89dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="3dp"
        android:background="#098FBA"
        android:text="SERVICIOS"
        app:layout_constraintEnd_toStartOf="@+id/button6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnfotos"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button6"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="166dp"
        android:layout_height="47dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="15dp"
        android:background="#098FBA"
        android:text="CONTACTOS"
        app:layout_constraintStart_toEndOf="@+id/button4"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button7"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="292dp"
        android:layout_height="44dp"
        android:layout_marginStart="93dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="26dp"
        android:background="#098FBA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        tools:ignore="MissingConstraints"
        tools:text="CERRAR SESION" />
</androidx.constraintlayout.widget.ConstraintLayout>